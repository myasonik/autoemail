mixin addBBBheader-550-head(campaignLink)
	+newRow
		td(width="550", height="10", valign="top")
			img(src="imgs/spacer.gif", width="550", height="10", alt=" ")
	+newRow
		// table right above should have width="550" added
		td(width="275", align="left", valign="bottom")
			p(style="font-size: 10px; font-family: Arial, Helvetica, sans-serif; color: #273691;")
				if block
					block
		td(width="275", align="right", valign="bottom")
			p(style="font-size: 10px; font-family: Arial, Helvetica, sans-serif; color: #273691;")
				+a('http://view.ed4.net/v/_ECLIENTID__/_ECELLID__/_EOLISTID__/_EOCID__/MAILACTION=_MAILACTION__&FORMAT=H?' + campaignLink)(style="color: #273691;") View as web page

